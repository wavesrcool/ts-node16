{
  "name": "ts-node16",
  "version": "0.0.1",
  "description": "A Typescript base in Node.js version 16",
  "main": "bin/index.js",
  "typings": "bin/index.d.ts",
  "author": "Tyson Lupul <wavesrcool@icloud.com>",
  "license": "MIT",
  "scripts": {
    "_0bin": "rm -rf bin",
    "_0tsc": "rm -f tsconfig.tsbuildinfo",
    "w": "tsc -w",
    "c": "yarn _0bin && yarn _0tsc && tsc",
    "build": "yarn && yarn lint && yarn c",
    "envs": "rm -f src/env.d.ts && gen-env-types .env -o src/env.d.ts -e .",
    "lint": "eslint --ext=.tsx,.ts src",
    "lint:fix": "eslint --cache --fix --ext=.tsx,.ts src",
    "start": "yarn envs && NODE_ENV=production node bin/index.js",
    "dev": "nodemon bin/index.js",
    "rename": "node --loader ts-node/esm rename.ts"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/dotenv-safe": "^8.1.2",
    "@types/node": "^18.7.8",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gen-env-types": "^1.3.4",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "dotenv-safe": "^8.2.0"
  }
}
